{
   "private": true,
  "name": "react-native-track-player",
  "version": "4.1.1",
  "description": "A fully fledged audio module created for music apps",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "web/**/*",
    "*.podspec",
    "react-native.config.js",
    "!docs/**/*",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "contributors": [
    {
      "name": "David Chavez",
      "email": "david@dcvz.io",
      "url": "http://dcvz.io"
    },
    {
      "name": "Guilherme Chaguri",
      "email": "guichaguri@gmail.com",
      "url": "http://guichaguri.com"
    },
    {
      "name": "Dustin Bahr",
      "email": "dustin@curiousmedia.com"
    }
  ],
  "scripts": {
    "example": "yarn workspace react-native-track-player-example", 
    "typecheck": "tsc",  
    "format:fix": "prettier --write src web",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "build": "yarn prepare",
    "prepare": "bob build"
  },
  "keywords": [
    "react",
    "react-native",
    "track-player",
    "audio-player",
    "audio",
    "player",
    "music",
    "controls",
    "chromecast",
    "android",
    "ios",
    "windows",
    "hooks"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/doublesymmetry/react-native-track-player.git"
  },
  "bugs": {
    "url": "https://github.com/doublesymmetry/react-native-track-player/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "shaka-player": "^4.7.9"
  },
  "peerDependenciesMeta": {
    "shaka-player": {
      "optional": true
    }
  },
  "devDependencies": { 
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0", 
    "@react-native-community/cli": "15.0.0-alpha.2",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "^0.78.0", 
    "@types/jest": "^29.5.5", 
    "react": "19.1.0",
    "react-native": "0.80.0",
    "react-native-builder-bob": "^0.40.12", 
    "shaka-player": "^4.15.4", 
    "typescript": "^5.8.3"
  },
  "funding": "https://github.com/doublesymmetry/react-native-track-player?sponsor=1",
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@3.6.1",
   
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "TrackPlayerSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.doublesymmetry.trackplayer"
    },
    "ios": {
      "modulesProvider": {
        "NativeTrackPlayer": "TrackPlayer"
      }
    }
  },
  "create-react-native-library": {
    "languages": "kotlin-objc",
    "type": "turbo-module",
    "version": "0.51.0"
  }
}